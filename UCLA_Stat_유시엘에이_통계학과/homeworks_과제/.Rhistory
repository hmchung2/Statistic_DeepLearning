# write your code here
ttt<- function(x){
if(missing(x)){
state <- as.character(1:9)
structure(state, class = "ttt")
}
else{
structure(x, class = "ttt")
}
}
print.ttt <- function(state){
cat("", state[1], "|", state[2], "|", state[3], "\n")
cat("---------", "\n")
cat("", state[4], "|", state[5], "|", state[6], "\n")
cat("---------", "\n")
cat("", state[7], "|", state[8], "|", state[9], "\n")
}
